package com.psl.prob2;

public class Prob2 
{
	static boolean checkAlpha(char alph)
	{
		alph = Character.toLowerCase(alph);
		if( alph=='a' || alph=='e' || alph=='i' || alph=='o' || alph=='u')
			return true;
		else
			return false;
	}
	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub

	}

}
