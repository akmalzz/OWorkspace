package com.psl.prob7;

public class Prob7 
{
	static float calculateProfit( int numAttendees)
	{
		float income = 0.0f;
		income = ( 5 * (float)numAttendees ) - (20+ (0.5 * (float)numAttendees));
		return income;
	}
